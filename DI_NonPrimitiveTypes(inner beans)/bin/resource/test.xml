<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN"
			"http://www.springframework.org/dtd/spring-beans-2.0.dtd">

<beans>
	<bean class="bean.Car" id="c1">
		<property name="carname" value="BMW xyz"></property>
		<property name="carcolor" value="white"></property>
		<property name="carcount" value="6"></property>
		<property name="e">
			<bean class="bean.Engine" id="e1">
				<property name="cc" value="3000cc"></property>
			</bean>
		</property>
	</bean>

	<bean class="bean.Car" id="c2">
		<property name="carname" value="Audi xyz"></property>
		<property name="carcolor" value="grey"></property>
		<property name="carcount" value="2"></property>
		<property name="e">
			<bean class="bean.Engine" id="e2">
				<property name="cc" value="3000cc"></property>
			</bean>
		</property>
	</bean>
</beans>